"WorldVertexTransition"
{
	"$surfaceprop" "water"
	"$basetexture" "water/mp3_water_lagoon01"
	"$basetexture2" "water/mp3_water_lagoon02"
	
	"%tooltexture" "water/mp3_water_lagoon02"
 
	"$bumpmap" "dev/water_normal"
	
	"$normalmap" "water/mp3_waterock_normal"

	"$bumptransform" "center .5 .5 scale 10 10 rotate 0  translate 0 0"

	"%keywords" "MP3;water"

	"$reflectamount" "1"
	"$reflecttint" "{255 255 255}"

	"$envmap" "env_cubemap"

	"$normalmapalphaenvmapmask" 1
	"$bumpbasetexture2withbumpmap" 1

	"$envmapcontrast" 1
	"$envmapsaturation" 1
	"$envmaptint" "[1 1 1]"

	"$bumpframe" "0"

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 10.00
		}

		"TextureScroll"
		{
			"texturescrollvar" "$bumptransform"
			"texturescrollrate" .03
			"texturescrollangle" 180.00
		}
	}
}
